#!bash
#
# This file might be sourced during installation with hsadmin
#

[ -e "${HOME}/.config/pep8" ] \
    || ln -fs "$(pwd)/.config-base/pep8" "${HOME}/.config/"

[ -e "${HOME}/.pulse/default.pa" ] \
    || (mkdir -p "${HOME}/.pulse" \
    && ln -fs "$(pwd)/.config-base/pulse/default.pa" "${HOME}/.pulse/" )
